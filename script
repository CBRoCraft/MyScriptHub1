local Toggle = ScriptsTab:CreateToggle({
   Name = "Every Second you get +1 health",
   CurrentValue = false,
   Flag = "Toggle1",
   Callback = function(Value)
      _G.enabled = Value

	  while _G.enabled do
	    wait()
			firetouchinterest(game:GetService("Workspace").Wins.Pipe,game.Players.LocalPlayer.Character.HumanoidRootPart,0)
	    wait()
		firetouchinterest(game:GetService("Workspace").Wins.Pipe,game.Players.LocalPlayer.Character.HumanoidRootPart,1)
	  end
   end,
})

local Toggle = ScriptsTab:CreateToggle({
   Name = "Anime Energy Clash Simulator",
   CurrentValue = false,
   Flag = "Toggle1",
   Callback = function(Value)
		_G.Win = Value
		_G.AutoClick = Value
		while _G.Win do
			local args = { 
    			[1] = true
			}		
			game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Event"):WaitForChild("Game"):WaitForChild("[C-S]PlayerPKEnd"):FireServer(unpack(args))
			wait()
	  	end
		while _G.AutoClick do
   			game:GetService("ReplicatedStorage"):WaitForChild("Remote"):WaitForChild("Event"):WaitForChild("Game"):WaitForChild("[C-S]PlayerClick"):FireServer()
		end
   end,
})

